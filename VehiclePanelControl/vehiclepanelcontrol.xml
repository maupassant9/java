<?xml version="1.0" encoding="UTF-8"?>
<project name="vehiclepanelcontrol" default="all">
  
  
  <property file="vehiclepanelcontrol.properties"/>
  <!-- Uncomment the following property if no tests compilation is needed -->
  <!-- 
  <property name="skip.tests" value="true"/>
   -->
  
  <!-- Compiler options -->
  
  <property name="compiler.debug" value="on"/>
  <property name="compiler.generate.no.warnings" value="off"/>
  <property name="compiler.args" value=""/>
  <property name="compiler.max.memory" value="700m"/>
  <patternset id="ignored.files">
    <exclude name="**/*.hprof/**"/>
    <exclude name="**/*.pyc/**"/>
    <exclude name="**/*.pyo/**"/>
    <exclude name="**/*.rbc/**"/>
    <exclude name="**/*.yarb/**"/>
    <exclude name="**/*~/**"/>
    <exclude name="**/.DS_Store/**"/>
    <exclude name="**/.git/**"/>
    <exclude name="**/.hg/**"/>
    <exclude name="**/.svn/**"/>
    <exclude name="**/CVS/**"/>
    <exclude name="**/__pycache__/**"/>
    <exclude name="**/_svn/**"/>
    <exclude name="**/vssver.scc/**"/>
    <exclude name="**/vssver2.scc/**"/>
  </patternset>
  <patternset id="library.patterns">
    <include name="*.egg"/>
    <include name="*.jar"/>
    <include name="*.ear"/>
    <include name="*.ane"/>
    <include name="*.klib"/>
    <include name="*.swc"/>
    <include name="*.war"/>
    <include name="*.zip"/>
  </patternset>
  <patternset id="compiler.resources">
    <exclude name="**/?*.java"/>
    <exclude name="**/?*.form"/>
    <exclude name="**/?*.class"/>
    <exclude name="**/?*.groovy"/>
    <exclude name="**/?*.scala"/>
    <exclude name="**/?*.flex"/>
    <exclude name="**/?*.kt"/>
    <exclude name="**/?*.clj"/>
  </patternset>
  
  <!-- JDK definitions -->
  
  <property name="jdk.bin.11" value="${jdk.home.11}/bin"/>
  <path id="jdk.classpath.11">
    <fileset dir="${jdk.home.11}">
      <include name="../jbr!/java.base"/>
      <include name="../jbr!/java.compiler"/>
      <include name="../jbr!/java.datatransfer"/>
      <include name="../jbr!/java.desktop"/>
      <include name="../jbr!/java.instrument"/>
      <include name="../jbr!/java.logging"/>
      <include name="../jbr!/java.management"/>
      <include name="../jbr!/java.management.rmi"/>
      <include name="../jbr!/java.naming"/>
      <include name="../jbr!/java.net.http"/>
      <include name="../jbr!/java.prefs"/>
      <include name="../jbr!/java.rmi"/>
      <include name="../jbr!/java.scripting"/>
      <include name="../jbr!/java.se"/>
      <include name="../jbr!/java.security.jgss"/>
      <include name="../jbr!/java.security.sasl"/>
      <include name="../jbr!/java.smartcardio"/>
      <include name="../jbr!/java.sql"/>
      <include name="../jbr!/java.sql.rowset"/>
      <include name="../jbr!/java.transaction.xa"/>
      <include name="../jbr!/java.xml"/>
      <include name="../jbr!/java.xml.crypto"/>
      <include name="../jbr!/javafx.base"/>
      <include name="../jbr!/javafx.controls"/>
      <include name="../jbr!/javafx.fxml"/>
      <include name="../jbr!/javafx.graphics"/>
      <include name="../jbr!/javafx.media"/>
      <include name="../jbr!/javafx.swing"/>
      <include name="../jbr!/javafx.web"/>
      <include name="../jbr!/jdk.accessibility"/>
      <include name="../jbr!/jdk.aot"/>
      <include name="../jbr!/jdk.attach"/>
      <include name="../jbr!/jdk.charsets"/>
      <include name="../jbr!/jdk.compiler"/>
      <include name="../jbr!/jdk.crypto.cryptoki"/>
      <include name="../jbr!/jdk.crypto.ec"/>
      <include name="../jbr!/jdk.dynalink"/>
      <include name="../jbr!/jdk.httpserver"/>
      <include name="../jbr!/jdk.internal.ed"/>
      <include name="../jbr!/jdk.internal.jvmstat"/>
      <include name="../jbr!/jdk.internal.le"/>
      <include name="../jbr!/jdk.internal.vm.ci"/>
      <include name="../jbr!/jdk.internal.vm.compiler"/>
      <include name="../jbr!/jdk.internal.vm.compiler.management"/>
      <include name="../jbr!/jdk.jdi"/>
      <include name="../jbr!/jdk.jdwp.agent"/>
      <include name="../jbr!/jdk.jfr"/>
      <include name="../jbr!/jdk.jsobject"/>
      <include name="../jbr!/jdk.localedata"/>
      <include name="../jbr!/jdk.management"/>
      <include name="../jbr!/jdk.management.agent"/>
      <include name="../jbr!/jdk.management.jfr"/>
      <include name="../jbr!/jdk.naming.dns"/>
      <include name="../jbr!/jdk.naming.rmi"/>
      <include name="../jbr!/jdk.net"/>
      <include name="../jbr!/jdk.pack"/>
      <include name="../jbr!/jdk.scripting.nashorn"/>
      <include name="../jbr!/jdk.scripting.nashorn.shell"/>
      <include name="../jbr!/jdk.sctp"/>
      <include name="../jbr!/jdk.security.auth"/>
      <include name="../jbr!/jdk.security.jgss"/>
      <include name="../jbr!/jdk.unsupported"/>
      <include name="../jbr!/jdk.xml.dom"/>
      <include name="../jbr!/jdk.zipfs"/>
    </fileset>
  </path>
  
  <property name="project.jdk.home" value="${jdk.home.11}"/>
  <property name="project.jdk.bin" value="${jdk.bin.11}"/>
  <property name="project.jdk.classpath" value="jdk.classpath.11"/>
  
  
  <!-- Project Libraries -->
  
  <path id="library.lib.classpath">
    <fileset dir="C:/Program Files/Java/javafx-sdk-11.0.2/lib">
      <patternset refid="library.patterns"/>
    </fileset>
  </path>
  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <fileset dir="${javac2.home}">
      <include name="javac2.jar"/>
      <include name="jdom.jar"/>
      <include name="asm-all*.jar"/>
      <include name="forms-*.jar"/>
    </fileset>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  
  <!-- Module VehiclePanelControl -->
  
  <dirname property="module.vehiclepanelcontrol.basedir" file="${ant.file}"/>
  
  
  <property name="module.jdk.home.vehiclepanelcontrol" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.vehiclepanelcontrol" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.vehiclepanelcontrol" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.vehiclepanelcontrol" value="-encoding UTF-8 -source 9 -target 9 ${compiler.args}"/>
  
  <property name="vehiclepanelcontrol.output.dir" value="${module.vehiclepanelcontrol.basedir}/out/production/VehiclePanelControl"/>
  <property name="vehiclepanelcontrol.testoutput.dir" value="${module.vehiclepanelcontrol.basedir}/out/test/VehiclePanelControl"/>
  
  <path id="vehiclepanelcontrol.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="vehiclepanelcontrol.module.production.classpath">
    <path refid="${module.jdk.classpath.vehiclepanelcontrol}"/>
    <path refid="library.lib.classpath"/>
  </path>
  
  <path id="vehiclepanelcontrol.runtime.production.module.classpath">
    <pathelement location="${vehiclepanelcontrol.output.dir}"/>
    <path refid="library.lib.classpath"/>
  </path>
  
  <path id="vehiclepanelcontrol.module.classpath">
    <path refid="${module.jdk.classpath.vehiclepanelcontrol}"/>
    <pathelement location="${vehiclepanelcontrol.output.dir}"/>
    <path refid="library.lib.classpath"/>
  </path>
  
  <path id="vehiclepanelcontrol.runtime.module.classpath">
    <pathelement location="${vehiclepanelcontrol.testoutput.dir}"/>
    <pathelement location="${vehiclepanelcontrol.output.dir}"/>
    <path refid="library.lib.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.vehiclepanelcontrol">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.vehiclepanelcontrol">
    <patternset refid="excluded.from.module.vehiclepanelcontrol"/>
  </patternset>
  
  <path id="vehiclepanelcontrol.module.sourcepath">
    <dirset dir="${module.vehiclepanelcontrol.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.vehiclepanelcontrol" depends="compile.module.vehiclepanelcontrol.production,compile.module.vehiclepanelcontrol.tests" description="Compile module VehiclePanelControl"/>
  
  <target name="compile.module.vehiclepanelcontrol.production" depends="register.custom.compilers" description="Compile module VehiclePanelControl; production classes">
    <mkdir dir="${vehiclepanelcontrol.output.dir}"/>
    <javac2 destdir="${vehiclepanelcontrol.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.vehiclepanelcontrol}/javac">
      <compilerarg line="${compiler.args.vehiclepanelcontrol}"/>
      <bootclasspath refid="vehiclepanelcontrol.module.bootclasspath"/>
      <classpath refid="vehiclepanelcontrol.module.production.classpath"/>
      <src refid="vehiclepanelcontrol.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.vehiclepanelcontrol"/>
    </javac2>
    
    <copy todir="${vehiclepanelcontrol.output.dir}">
      <fileset dir="${module.vehiclepanelcontrol.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.vehiclepanelcontrol.tests" depends="register.custom.compilers,compile.module.vehiclepanelcontrol.production" description="compile module VehiclePanelControl; test classes" unless="skip.tests"/>
  
  <target name="clean.module.vehiclepanelcontrol" description="cleanup module">
    <delete dir="${vehiclepanelcontrol.output.dir}"/>
    <delete dir="${vehiclepanelcontrol.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.vehiclepanelcontrol, clean.artifact.vehiclepanelcontrol" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.vehiclepanelcontrol" description="build all modules"/>
  
  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.vehiclepanelcontrol" value="${basedir}/out/artifacts/VehiclePanelControl"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="artifact.temp.output.VehiclePanelControl" value="${artifacts.temp.dir}/VehiclePanelControl.jar"/>
    <taskdef resource="com/sun/javafx/tools/ant/antlib.xml" uri="javafx:com.sun.javafx.tools.ant" classpath="${jdk.home.11}/lib/ant-javafx.jar"/>
  </target>
  
  <target name="clean.artifact.vehiclepanelcontrol" description="clean VehiclePanelControl artifact output">
    <delete dir="${artifact.output.vehiclepanelcontrol}"/>
  </target>
  
  <target name="artifact.vehiclepanelcontrol" depends="init.artifacts, compile.module.vehiclepanelcontrol" description="Build &#39;VehiclePanelControl&#39; artifact" xmlns:fx="javafx:com.sun.javafx.tools.ant">
    <mkdir dir="${artifact.output.vehiclepanelcontrol}"/>
    <copy todir="${artifact.temp.output.VehiclePanelControl}">
      <fileset dir="${vehiclepanelcontrol.output.dir}"/>
    </copy>
    <fx:fileset id="all_but_VehiclePanelControl" dir="${artifact.temp.output.VehiclePanelControl}" includes="**/*.jar">
      <exclude name="VehiclePanelControl.jar"/>
    </fx:fileset>
    <fx:fileset id="all_VehiclePanelControl" dir="${artifact.temp.output.VehiclePanelControl}" includes="**/*.jar"/>
    <fx:application id="VehiclePanelControl_id" name="VehiclePanelControl" mainClass="vehiclepanel.Main"/>
    <fx:jar destfile="${artifact.temp.output.VehiclePanelControl}/VehiclePanelControl.jar">
      <fx:application refid="VehiclePanelControl_id"/>
      <fileset dir="${artifact.temp.output.VehiclePanelControl}" excludes="**/*.jar"/>
      <fx:resources>
        <fx:fileset refid="all_but_VehiclePanelControl"/>
      </fx:resources>
      <manifest>
        <attribute name="Implementation-Title" value=""/>
        <attribute name="Implementation-Version" value=""/>
        <attribute name="Implementation-Vendor" value=""/>
      </manifest>
    </fx:jar>
    <fx:deploy width="600" height="400" updatemode="background" outdir="${artifact.temp.output.VehiclePanelControl}/deploy" outfile="VehiclePanelControl">
      <fx:application refid="VehiclePanelControl_id"/>
      <fx:resources>
        <fx:fileset refid="all_VehiclePanelControl"/>
      </fx:resources>
    </fx:deploy>
    <copy todir="${artifact.output.vehiclepanelcontrol}">
      <fileset dir="${artifact.temp.output.VehiclePanelControl}/deploy"/>
    </copy>
    <delete includeemptydirs="true">
      <fileset dir="${artifact.temp.output.VehiclePanelControl}"/>
    </delete>
  </target>
  
  <target name="build.all.artifacts" depends="artifact.vehiclepanelcontrol" description="Build all artifacts">
    
    <!-- Delete temporary files -->
    <delete dir="${artifacts.temp.dir}"/>
  </target>
  
  <target name="all" depends="build.modules, build.all.artifacts" description="build all"/>
</project>